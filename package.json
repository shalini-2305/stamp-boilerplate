{
  "name": "x-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.13.0",
    "pnpm": ">=9.15.3"
  },
  "engineStrict": true,
  "packageManager": "pnpm@9.15.3",
  "scripts": {
    "dev:wp": "pnpm with-env next dev",
    "build": "set NODE_ENV=production&& pnpm with-env next build",
    "start": "pnpm with-env next start",
    "format": "prettier --check . --cache --cache-location .cache/.prettiercache",
    "format:fix": "prettier --write . --cache --cache-location .cache/.prettiercache",
    "lint": "eslint . --cache --cache-location .cache/.eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location .cache/.eslintcache",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "postinstall": "pnpm convert-fonts",
    "precommit:fix": "pnpm format:fix && pnpm lint:fix && pnpm typecheck",
    "clean": "git clean -xdf .cache .next node_modules",
    "with-env": "dotenv -e .env --"
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@auth/drizzle-adapter": "^1.10.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "catalog:",
    "@t3-oss/env-core": "^0.13.8",
    "@t3-oss/env-nextjs": "catalog:",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:",
    "@trpc/react-query": "catalog:",
    "@trpc/server": "catalog:",
    "@vercel/postgres": "catalog:",
    "canvas": "^3.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "catalog:",
    "drizzle-zod": "0.5.1",
    "geist": "^1.3.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "next": "15.1.6",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.4.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.60.0",
    "react-konva": "^19.0.2",
    "sonner": "^1.7.0",
    "superjson": "2.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "catalog:",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.4",
    "eslint": "catalog:",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "husky": "^9.1.7",
    "jiti": "^1.21.6",
    "postcss": "^8.4.32",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "catalog:",
    "tw-animate-css": "^1.3.5",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "@next/env": "15.1.6"
    }
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
